N * 	@arg\b			SPI0.
N * @param[in]   SPI_Flag
N * 	@arg\b			SPI_BSYF
N * 	@arg\b			SPI_IDLF
N * 	@arg\b			SPI_TCF
N * 	@arg\b			SPI_RXDF
N * 	@arg\b			SPI_RXF
N * 	@arg\b			SPI_TXF
N * 	@arg\b			SPI_MODF
N * 	@arg\b			SPI_WEF
N * 	@arg\b			SPI_ROVRF
N * 	@arg\b			SPI_TUDRF
N *  @arg\b          SPI_IDL_STA
N * @return      DRV_Return
N * 	@arg\b			DRV_Normal
N * 	@arg\b			DRV_Happened
N * @note
N * @par         Example
N * @code
N    SPI_GetSingleFlagStatus(SPI0, SPI_TXF);
N * @endcode
N *******************************************************************************
N */
NDRV_Return SPI_GetSingleFlagStatus (SPI_Struct* SPIx, uint32_t SPI_Flag)
N{
N    
N    DRV_Return bitstatus = DRV_Normal;
N    
N    if((SPIx->STA.W & SPI_Flag) != 0)
N    {
N        bitstatus = DRV_Happened; 
N    }
N    
N    return bitstatus;
N}
