N * 	@arg\b			SPI_INT_ROVR
N * 	@arg\b			SPI_INT_TUDR
N * @param[in]   NewState
N * 	@arg\b          DISABLE
N * 	@arg\b          ENABLE
N * @return	    	
N * @note
N * @par         Example
N * @code
N    SPI_IT_Config(SPI0, SPI_INT_TC | SPI_INT_RX, ENABLE);
N * @endcode
N *******************************************************************************
N */
Nvoid SPI_IT_Config (SPI_Struct* SPIx, uint32_t SPI_INTS, FunctionalState NewState)
N{
N    if(NewState != DISABLE)
N    {
N        SPIx->INT.W |= SPI_INTS;
